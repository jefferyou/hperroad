    "weight_decay": 1e-5,
    "grad_accmu_steps": 1,
    "clip_grad_norm": true,
    "max_grad_norm": 1.0,
    "use_early_stop": true,
    "patience": 50,
    "log_every": 1,
    "saved_model": true,
    "load_best_epoch": true,
    "hyper_tune": false,
    "_comment": "Executor configuration for HRNR_Hyperbolic with learning rate warmup and cosine annealing"
}
