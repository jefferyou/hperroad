{
    "output_dim": 128,
    "max_epoch": 100,
    "learner": "adam",
    "learning_rate": 1e-4,
    "lr_scheduler": "cosinelr",
    "lr_decay": true,
    "lr_decay_ratio": 0.1,
    "lr_alpha": 0.1,
    "lr_beta": 0.75,
    "lr_step": 10,
    "lr_warmup": true,
    "lr_warmup_epoch": 5,
